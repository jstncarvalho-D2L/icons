{
  "description": "SVG images, Polymer iron-iconset collections and SASS mixins for D2L icons",
  "private": true,
  "keywords": [
    "d2l",
    "ui",
    "icons",
    "scss",
    "sass",
    "mixins",
    "polymer",
    "web component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/icons.git"
  },
  "homepage": "https://github.com/BrightspaceUI/icons",
  "name": "d2l-icons",
  "scripts": {
    "build": "npm run build:scss && npm run build:iconsets",
    "build:scss": "node ./cli/create-scss-cli -o icons.scss images/**/*.*",
    "build:iconsets": "node ./cli/create-iconset-cli images/tier1 images/tier2 images/tier3 images/html-editor images/emoji",
    "lint": "npm run lint:html && npm run lint:wc",
    "lint:html": "eslint *.js *.html demo/*.html cli/*.js",
    "lint:wc": "polymer lint --npm *.js",
    "test": "npm run lint && npm run build && npm run test:polymer:local",
    "test:polymer:local": "polymer test --npm --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --npm --skip-plugin local"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/BrightspaceUI/icons/issues"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "^3.0.0-pre.18",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.0.2",
    "babel-eslint": "^8.2.5",
    "chalk": "^1.1.1",
    "d2l-typography": "git+https://github.com/BrightspaceUI/typography.git#polymer-3",
    "eslint": "^4.15.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.1",
    "fs": "0.0.2",
    "imagemin": "^4.0.0",
    "images-to-variables": "^0.3.0",
    "polymer-cli": "^1.7.7",
    "q": "^1.4.1",
    "through2": "^2.0.1",
    "vinyl-fs": "^2.3.1",
    "wct-browser-legacy": "^0.0.1-pre.11",
    "xml2js": "^0.4.16",
    "yargs": "^4.1.0"
  },
  "version": "6.0.0",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0",
    "@webcomponents/webcomponentsjs": "2.0.0-beta.2"
  },
  "main": "d2l-icons.js",
  "dependencies": {
    "@polymer/iron-iconset-svg": "^3.0.0-pre.18",
    "@polymer/polymer": "^3.0.0",
    "d2l-colors": "git+https://github.com/BrightspaceUI/colors.git#polymer-3"
  }
}
